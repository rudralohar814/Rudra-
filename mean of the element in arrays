#include<stdio.h>
int main(){
    int n;
    printf("Enter the number of the element you wnat to enter:-");
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    int sum=0;
    for(int i=0;i<n;i++){
        sum=sum+arr[i];
    }
    int mean = sum/n;
    printf(" The mean of the elment is:- %d",mean);
    return 0;
}
